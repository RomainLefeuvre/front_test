    "test:e2e:browser:headed": "playwright test --headed",
    "check-minio": "node scripts/checkMinio.js",
    "setup-test-data": "node scripts/setupTestData.js",
    "extract-test-subset": "node scripts/extractTestSubset.js",
    "setup-minio-e2e": "bash scripts/setupMinioE2E.sh",
    "setup-minio-dev": "bash scripts/setupMinioDev.sh",
