    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/tar": "^6.1.13",
    "@vitejs/plugin-react": "^5.1.1",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fast-check": "^4.3.0",
    "fflate": "^0.8.2",
    "globals": "^16.5.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tar": "^7.5.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
